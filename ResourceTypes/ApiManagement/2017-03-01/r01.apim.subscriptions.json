{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "apiName": {
            "type": "string",
            "metadata": {
                "description": "Required. The apim name."
            }
        },
        "users": {
            "type": "string",
            "metadata": {
                "description": "Required. The users name."
            }
        },
        "products": {
            "type": "string",
            "metadata": {
                "description": "Required. The products name."
            }
        },
        "subscriptionsName": {
            "type": "string",
            "metadata": {
                "description": "Name of subscriptions"
            }
        },
        "displayName": {
            "type": "string",
            "metadata": {
                "description": "Required. The displayName."
            }
        }
    },
    "functions": [
    ],
    "variables": {
    },
    "resources": [
        {
            "type": "Microsoft.ApiManagement/service/subscriptions",
            "apiVersion": "2017-03-01",
            "name": "[concat(parameters('apiName'), '/', parameters('subscriptionsName'))]",
            "dependsOn": [],
            "properties": {
                "productId": "[resourceId('Microsoft.ApiManagement/service/products/' parameters('apiName') parameters('products'))]",
                "userId": "[resourceId('Microsoft.ApiManagement/service/users/' parameters('apiName') '/'parameters('users'))]",
                "scope": "[resourceId('Microsoft.ApiManagement/service/products/' parameters('apiName') '/' parameters('products'))]",
                "displayName": "[parameters('displayName')]",
                "state": "active",
                "allowTracing": false
            }
        }
    ],
    "outputs": {
    }
}